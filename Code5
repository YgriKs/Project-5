using System;

class Program {
    static void Main(string[] args) {
		Console.WriteLine("Введите число которое соответствует дню недели");
		int a = Convert.ToInt32(Console.ReadLine());
			switch(a)
				{
					case 1:
					case 2:
					case 3:
					case 4:
						Console.WriteLine("Младший класс");
					break;
						  
					case 5:
					case 6:
					case 7:
					case 8:
					case 9:
						Console.WriteLine("Средний класс");
					break;

					case 10:
					case 11:
						Console.WriteLine("Старший класс");
					break;

				default:
				Console.WriteLine("Такого класса нет");
					break;
						  }
    }
}
